func add(x: felt, y: felt) -> felt {
    return x + y;

    end:
}

func sub(x: felt, y: felt) -> felt {
    return x - y;

    end:
}

func mul(x: felt, y: felt) -> felt {
    return x * y;

    end:
}

func div(x: felt, y: felt) -> felt {
    return x / y;

    end:
}

func diff_ratio(x: felt, y: felt) -> felt {
    return (x - y) / (x - y);

    end:
}

func sum_ratio(x: felt, y: felt) -> felt {
    return (x + y) / (x + y);

    end:
}
